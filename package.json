{
  "name": "reservation-service",
  "version": "0.0.1",
  "description": "reservation-service",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16 || 18 || 20 || 22 || 23"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build . -t reservation-service:$(npm pkg get version | xargs echo)",
    "docker:run": "docker run -p 3000:80 -d reservation-service:$(npm pkg get version | xargs echo)",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Ojvar <MOjvar.AmirHossein@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^7.0.8",
    "@loopback/context": "^7.0.8",
    "@loopback/core": "^6.1.5",
    "@loopback/repository": "^7.0.8",
    "@loopback/rest": "^14.0.8",
    "@loopback/rest-explorer": "^7.0.8",
    "@loopback/service-proxy": "^7.0.8",
    "@sentry/node": "^8.41.0",
    "@sentry/profiling-node": "^8.41.0",
    "amqplib": "^0.10.5",
    "bson": "^6.10.0",
    "dotenv": "^16.4.5",
    "jssha": "^3.3.1",
    "keycloak-connect": "^22.0.5",
    "loopback-connector-mongodb": "^6.3.0",
    "loopback-connector-rest": "^5.0.2",
    "mime": "^4.0.4",
    "mssql": "^10.0.4",
    "redis": "^4.7.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.7",
    "@loopback/eslint-config": "^15.0.4",
    "@loopback/testlab": "^7.0.7",
    "@types/amqplib": "^0.10.5",
    "@types/mssql": "^9.1.5",
    "@types/node": "^16.18.121",
    "@types/redis": "^4.0.11",
    "eslint": "^8.57.1",
    "husky": "^8.0.3",
    "pinst": "^3.0.0",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2"
  }
}
